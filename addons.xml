<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.lastship.common" name="Lastship Libraries Repository" version="1.0.4" provider-name="Lastship Team">
    <extension point="xbmc.addon.repository" name="Libraries Repository">
        <info compressed="false">https://github.com/complexprof/lastship-common/raw/master/addons.xml</info>
        <checksum>https://github.com/complexprof/lastship-common/raw/master/addons.xml.md5</checksum>
        <datadir zip="true">https://github.com/complexprof/lastship-common/raw/master/zips/</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Faster Updating of Common Libraries for Kodi</summary>
        <description>This repository contains intermediate builds of libraries commonly used by Lastship Kodi Addons</description>
        <platform>all</platform>
    </extension>
</addon>

<addon id="script.module.urlresolver" name="URLResolver" version="4.0.53" provider-name="Resolver Team">
	<requires>
		<import addon="xbmc.python" version="2.1.0" />
		<import addon="plugin.video.youtube" optional="true" />
		<import addon="script.module.python.twitch" optional="true" />
	</requires>
	<extension point="xbmc.python.module" library="lib" />
	<extension point="xbmc.python.pluginsource" library="lib/default.py" />
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="de">URLResolver wird für die Stream Wiedergabe benötigt</summary>
		<description lang="de">Empfängt viele Video URL's, die in XBMC/Kodi abspielbar sind</description>
	</extension>
</addon>

<addon id="script.module.tulip" name="Tulip Routine Libraries" version="1.0.8" provider-name="Twilight0">
	<requires>
		<import addon="xbmc.python" version="2.24.0"/>
	</requires>
	<extension point="xbmc.python.module" library="lib" />
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Tulip Helper module</summary>
		<description lang="en">Tulip Routine Libraries are based on Lambda's "lamlib". It has some additions, most being in control module.</description>
		<platform>all</platform>
		<license>GNU GENERAL PUBLIC LICENSE. Version 3, 29 June 2007</license>
		<source>https://github.com/Twilight0/script.module.tulip</source>
	</extension>
</addon>
	
<addon id="script.module.requests" name="requests" version="2.12.4" provider-name="kennethreitz, beenje">
       <requires>
              <import addon="xbmc.python" version="2.14.0"/>
       </requires>
       <extension point="xbmc.python.module" library="lib" />
       <extension point="xbmc.addon.metadata">
              <summary>Python HTTP for Humans</summary>
              <description>Packed for KODI from https://github.com/kennethreitz/requests</description>
              <platform>all</platform>
              <language></language>
              <license>Apache2</license>
              <forum></forum>
              <website>http://python-requests.org</website>
	</extension>
</addon>
</addons>
